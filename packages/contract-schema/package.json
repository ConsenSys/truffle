{
  "name": "@truffle/contract-schema",
  "description": "JSON schema for contract artifacts",
  "license": "MIT",
  "author": "Tim Coulter <tim@trufflesuite.com>",
  "homepage": "https://github.com/trufflesuite/truffle/tree/master/packages/contract-schema#readme",
  "repository": "https://github.com/trufflesuite/truffle/tree/master/packages/contract-schema",
  "bugs": {
    "url": "https://github.com/trufflesuite/truffle/issues"
  },
  "version": "3.2.5",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "directories": {
    "spec": "./spec"
  },
  "scripts": {
    "build": "yarn build:js && yarn build:declarations",
    "build:js": "ttsc",
    "build:declarations": "sh ./scripts/generate-declarations",
    "prepare": "yarn run build",
    "test": "yarn prepare && mocha"
  },
  "typings": "./typings/index.d.ts",
  "dependencies": {
    "ajv": "^6.10.0",
    "crypto-js": "^3.1.9-1",
    "debug": "^4.1.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@types/sinon": "^9.0.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "json-schema-to-typescript": "^5.5.0",
    "mocha": "8.1.2",
    "solc": "0.5.16",
    "ttypescript": "^1.5.12"
  },
  "keywords": [
    "artifacts",
    "contract",
    "ethereum",
    "json",
    "schema"
  ],
  "publishConfig": {
    "access": "public"
